SELECT * FROM KOPO_CHANNEL_SEASONALITY;


CREATE TABLE SEASONALITY22 AS(
SELECT REGIONID
        ,PRODUCTGROUP
        ,PRODUCT
        ,YEARWEEK
        ,YEAR
        ,LPAD(WEEK,2,'0') AS WEEK
        ,QTY
        FROM KOPO_CHANNEL_SEASONALITY);
        
SELECT * FROM SEASONALITY22;

DESC SEASONALITY22;

DESC KOPO_CHANNEL_SEASONALITY;